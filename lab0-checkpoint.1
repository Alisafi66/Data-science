   "source": [
    "#Write your code here\n",
    "def starts_with_AL(word):\n",
    "    # Check if the length of the word is at least 2\n",
    "    if len(word) >= 2:\n",
    "        # Check if the first character is 'A' and the second character is 'L'\n",
    "        if word[0] == 'A' and word[1] == 'L':\n",
    "            return True\n",
    "    return False\n",
    "\n",
    "# Example usage:\n",
    "word1 = \"ALphabet\"\n",
    "word2 = \"ALien\"\n",
